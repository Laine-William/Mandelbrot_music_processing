// Creation de la class Sphere
class Sphere {
  
  // Variable radius, theta et phi sont de types float
  float radius, theta, phi;
  
  // Creation d'un constructeur Sphere avec 3 parametres radius, theta et phi, qui sont de types float
  Sphere (float radius, float theta, float phi) {
    
    // Radius
    this.radius = radius;
    
    // Theta
    this.theta = theta;
    
    // Phi
    this.phi = phi;
  }
}
